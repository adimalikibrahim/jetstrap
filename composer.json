{
  "name": "adimalikibrahim/jetstrap",
  "description": "A Laravel 10 package to easily switch TailwindCSS resources generated by Laravel Jetstream to Bootstrap 5.",
  "keywords": ["laravel", "bootstrap", "jetstream"],
  "license": "MIT",
  "authors": [
    {
      "name": "Pixel1",
      "email": "hello@pixel1.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "laravel/jetstream": "^4.0",
    "orchestra/testbench": "^8.3.0",
    "phpunit/phpunit": "^10.1"
  },
  "autoload": {
    "psr-4": {
      "Pixel1\\Jetstrap\\": "src/Pixel1/Jetstrap/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pixel1\\Jetstrap\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pixel1\\Jetstrap\\JetstrapServiceProvider"
      ],
      "aliases": {
        "Jetstrap": "Pixel1\\Jetstrap\\JetstrapFacade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "tests": "vendor/bin/phpunit",
    "testsVerbose": "@tests -vvv"
  }
}